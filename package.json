{
  "name": "simple-cognito-server-auth",
  "version": "1.0.2",
  "description": "A simple wrapper around Amazon's SDK library allowing to authenticate and create users from a backend server ",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "dev": "ts-node src/index.ts",
    "lint": "npx prettier --write './src/*' && npx tslint './src/*'",
    "build": "rimraf lib/; tsc"
  },
  "dependencies": {
    "aws-sdk": "^2.597.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.2",
    "optional-typescript": "^1.3.3",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.5",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/node": "^10.3.2",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslib": "^1.9.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.0.3"
  }
}
